Read a number and print number is positive number or negative number or zero

#include <stdio.h>

int main() {
    int a;
    scanf("%d",&a);
    printf("enter number");
    if(a>0)
    {
        printf("+ve no");
    }
    else if(a<0)
    {
        printf("-ve no");
    }
    else
    {
        printf("zero");
    }

    return 0;
}
